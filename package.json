{
  "name": "flask-react-demo",
  "version": "1.0.0",
  "description": "Flask, SQLAlchemy, and React.js Demo",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --config webpack.config.dev.js",
    "lint": "esw client",
    "lint:watch": "npm run lint -- --watch",
    "remove-dist": "rimraf ./dist",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist",
    "build": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "postbuild": "NODE_ENV=production node bin/dist-server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eddowh/flask-react-demo.git"
  },
  "author": "Eddo W. Hintoso",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/eddowh/flask-react-demo/issues"
  },
  "homepage": "https://github.com/eddowh/flask-react-demo#readme",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.0",
    "eslint-plugin-react": "^5.2.2",
    "eslint-watch": "^2.1.13",
    "npm-run-all": "^2.3.0",
    "open": "0.0.5",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.3",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
